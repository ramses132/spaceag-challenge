#: challenge/templates/account/password_reset.html:7
#: challenge/templates/account/password_reset.html:11
#: challenge/templates/account/password_reset_done.html:6
#: challenge/templates/account/password_reset_done.html:9
msgid "Password Reset"
msgstr "Reiniciar contraseña"

#: challenge/templates/account/password_reset.html:16
msgid ""
"Forgotten your password? Enter your e-mail address below, and we'll send you "
"an e-mail allowing you to reset it."
msgstr ""
"Has olvidado tu contraseña? Ingresa tu dirección de correo electrónico acá "
"abajo y enviaremos un email para que puedas reiniciar tu contraseña."

#: challenge/templates/account/password_reset.html:21
msgid "Reset My Password"
msgstr "Reiniciar mi contraseña"

#: challenge/templates/account/password_reset.html:24
msgid "Please contact us if you have any trouble resetting your password."
msgstr ""
"Porfavor contactanos si tu tienes algun problema reiniciando tu contraseña."

#: challenge/templates/account/password_reset_done.html:15
msgid ""
"We have sent you an e-mail. Please contact us if you do not receive it "
"within a few minutes."
msgstr ""
"hemos enviado un correo electronico. Porfavor contactanos si no lo recibes "
"en unos minutos."

#: challenge/templates/account/password_reset_from_key.html:8
msgid "Bad Token"
msgstr "Token Invalido"

#: challenge/templates/account/password_reset_from_key.html:12
#, python-format
msgid ""
"The password reset link was invalid, possibly because it has already been "
"used.  Please request a <a href=\"%(passwd_reset_url)s\">new password reset</"
"a>."
msgstr ""
"El link para reinicio de contraseña es invalido, quizas ya ha sido usado. "
"porfavor solicita un <a href=\"%(passwd_reset_url)s\">nuevo reinicio de "
"contraseña</a>."

#: challenge/templates/account/password_reset_from_key.html:18
msgid "change password"
msgstr "cambiar contraseña"

#: challenge/templates/account/password_reset_from_key.html:21
#: challenge/templates/account/password_reset_from_key_done.html:8
msgid "Your password is now changed."
msgstr "Tu contraseña ha sido cambiada."

#: challenge/templates/account/password_set.html:6
#: challenge/templates/account/password_set.html:9
#: challenge/templates/account/password_set.html:14
msgid "Set Password"
